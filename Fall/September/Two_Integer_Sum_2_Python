class Solution:
    def twoSum(self, numbers: List[int], target: int) -> List[int]:
        l = 0
        r = len(numbers)-1
        #1,2,3,4], target = 3
        res = []
        while l<r:
            addition = numbers[l] + numbers[r]
            print(addition)
            if addition < target :
                l+=1
            elif addition > target:
                r-=1
            else:

                return [l+1, r+1]
        
 

        
