class Solution:
    def addBinary(self, a: str, b: str) -> str:
        # Convert binary strings to integers
        AValue = int(a, 2)
        BValue = int(b, 2)
        
        # Add the two integers
        total = AValue + BValue
        
        # Convert the result back to binary and return (strip the "0b" prefix)
        return bin(total)[2:]

